import java.util.*;

public class Main {	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);		
		int n = sc.nextInt();
		List<Integer>measureList = new ArrayList<Integer>();
		
		for(int i=0; i<n; i++)
			measureList.add(sc.nextInt());
		
		measureList.sort(null);
		
		System.out.println(measureList.get(0)*measureList.get(measureList.size()-1));		
	}
}
