import java.util.*;

public class Main {
	static int factorial(int x) {
		if(x==0)
			return 1;
		else if(x==1)
			return 1;
		else
			return x * factorial(x-1);
	}
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		System.out.println(factorial(n));		
	}
}
