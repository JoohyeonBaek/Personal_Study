import java.util.*;

public class Main {
	static int fibonacci(int x) {
		if(x==0)
			return 0;
		else if(x==1)
			return 1;
		else
			return fibonacci(x-2) + fibonacci(x-1);
	}
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		System.out.println(fibonacci(n));		
	}
}
